
public class Client {
	public String nom;
	public double prix;
	
	public Client(String nom, String commande) {
		
		
		
		
			
	}

	public String getNom() {
		return nom;
	}

	public void setNom(String nom) {
		Client.nom = nom;
	}

	public double getPrix() {
		return prix;
	}

	public void setPrix(double prix) {
		Client.prix = prix;
	}
	
}

