
public class Plat {
	
	public String[] listePlats;
	
	
	public Plat(String listePlats) {
		
		String[] tabPlats;
		
		tabPlats = listePlats.split(" ");
		setListePlats(tabPlats);
		
	}


	public String[] getListePlats() {
		return listePlats;
	}


	public void setListePlats(String[] listePlats) {
		this.listePlats = listePlats;
	}
	
}
